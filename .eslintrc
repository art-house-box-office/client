{
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  },
  "extends": "airbnb",
  "parser": "babel-eslint",
  "rules": {
    "no-console": 0,
    "no-use-before-define": 0,
    "padded-blocks": 0,
    "no-trailing-spaces": 0,
    "no-param-reassign": 0,
    "no-return-assign": 0,
    "no-else-return": 0
  },
  "root": true
}
